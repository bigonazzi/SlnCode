{
    "name": "slncode",
    "displayName": "SlnCode",
    "description": "Edit visual studio solution files",
    "version": "0.0.1",
    "publisher": "bigonazzi",
    "license": "Apache-2.0",
    "engines": {
        "vscode": "^1.21.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:slncode.cloneProject"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "slncode.cloneProject",
                "title": "Clone Project",
                "category": "Solution"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "command": "slncode.cloneProject",
                    "when": "resourceExtname == .sln"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "dependencies": {
        "@types/uuid": "^3.4.3"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/bigonazzi/SlnCode.git"
    }
}
